    status
    time
    text
    url
  }

  # Navigation to different page for more reload tests
  goto(url: "https://httpbin.org/delay/1") {
    status
    url
  }

  # Commit-based reload (fastest)
  commitReload: reload(waitUntil: commit) {
    status
    time
    url
  }

